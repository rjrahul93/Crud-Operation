<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https:jcp.org/xml/ns/persistence
             http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
             version="2.1">

<persistence-unit name="Rahul">
    <provider>org.hiberate.jpa.HibernatePersistenceProvider</provider>
    <properties>
    
    <properties name="javax.persistence.jdbc.driver" value="com.mysql.ci.jdbe.Driver"/>
    
    <properties name="javax.persistence.jdbc.url" value="jdbc:mysql//localhost:3306/onetomany_unircreateDatabaseIfNotBxist=true"/>
    
    <properties name="javax.persistence.jdbc.user" value="root"/>
    <properties name="javax.persistence.jdbc.password" value="root"/>
    
    <properties name="hibernate.show_sql" value="true"/>
    <properties name="hibernate.hbm2dd1.auto" value="update"/>

    
    </properties>
</persistence-unit>

</persistence>



